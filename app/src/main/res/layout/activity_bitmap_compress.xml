<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#22000000"
    tools:context=".bitmap.BitmapCompressActivity">

    <Button
        android:id="@+id/btn_png8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="png8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="png8" />

    <Button
        android:id="@+id/btn_origin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="btn_origin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_quality"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="btn_quality"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_origin" />

    <Button
        android:id="@+id/btn_options"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="btn_options"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_quality" />

    <Button
        android:id="@+id/btn_matrix"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="btn_matrix"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_options" />


    <Button
        android:id="@+id/btn_rgb565"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="btn_rgb565"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_matrix" />

    <Button
        android:id="@+id/btn_rgb565_copy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="btn_rgb565_copy"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_rgb565" />

    <Button
        android:id="@+id/btn_png24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="png24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_png8" />

    <Button
        android:id="@+id/btn_png32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="png32"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_png24" />

    <Button
        android:id="@+id/btn_jpg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="jpg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_png32" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>